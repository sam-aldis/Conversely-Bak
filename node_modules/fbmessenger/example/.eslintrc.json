{
  "rules": {
    "no-console": 0
  },
  "env": {
    "node": true,
    "es6": true
  },
  "extends": ["airbnb-base"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "generators": true,
      "experimentalObjectRestSpread": true
    }
  }
}
